# program for checking parenthesis in pair, if are parenthesis in pair it return true otherwise false.  
def wellbracketed(str):
        count = 0
        for i in str:
                if i =="(":
                        count = count+1
                elif i ==")":
                        count = count -1
        if count == 0:
                return(True)
        else:
                return(False)
