# this is the program to rotate list k times, only fir if k is int, otherwise it returns 'list unchanges'
def rotatelist(list,k):
    if k >= 0:
        list1 = list[:]
        length = len(list)
        i = 0
        for each in list:
            index = i+k
            while index >= length:
                index = index-length
            list1[index] = each
            i = i+1
        return(list1)
    else:
        return('list unchanged')
